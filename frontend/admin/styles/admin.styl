.admin-workspace
  color: $textColor
  margin: auto
  padding-top: 30px

  *
    transition none

.admin-page
  top: 50px
  margin: auto

.float-left
  float left

.float-right
  float right

.max-width
  width: 100%

.not-allowed
  cursor: not-allowed

.reload
  height 20px
  width 20px
  border-radius 8px
  padding 1px
  margin 1px

  img
    position relative
    top -3px

.hidden-submit
  display none

.center
  margin auto
  text-align center
