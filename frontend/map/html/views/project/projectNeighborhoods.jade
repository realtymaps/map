.collapse-header
  h2.title Neighbourhoods ({{project.neighbourhoods.length}})
li(ng-repeat="neighbourhood in project.neighbourhoods" class="col-md-6")
  a(class="map" href="#/neighbourhoods/{{neighbourhood.id}}")
    span View Neighbourhood
    img(src="/assets/test_map.jpg")
  .data
    h3 {{neighbourhood.name}}
  button.btn.btn-link
    i.fa.fa-trash-o
    span Delete
