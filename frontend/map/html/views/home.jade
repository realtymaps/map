.welcome(class="{{pageClass}}" ng-show="getIsReady()" ng-cloak ng-class="welcomeClass")
  section
    .hero-container.center-on-page
      a.main(ui-sref="map",title="Show me the map!")
        .hero-container
          img(rmaps-cdn-image ng-src="/assets/logo.png")
