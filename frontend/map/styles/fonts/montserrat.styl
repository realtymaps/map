@font-face {
  font-family: 'montserratbold';
  src: url('/fonts/montserrat/montserrat_700-webfont.eot');
  src: url('/fonts/montserrat/montserrat_700-webfont.eot?#iefix') format('embedded-opentype'),
          url('/fonts/montserrat/montserrat_700-webfont.woff2') format('woff2'),
          url('/fonts/montserrat/montserrat_700-webfont.woff') format('woff'),
          url('/fonts/montserrat/montserrat_700-webfont.ttf') format('truetype'),
          url('/fonts/montserrat/montserrat_700-webfont.svg#montserratbold') format('svg');
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: 'montserratregular';
  src: url('/fonts/montserrat/montserrat_regular-webfont.eot');
  src: url('/fonts/montserrat/montserrat_regular-webfont.eot?#iefix') format('embedded-opentype'),
          url('/fonts/montserrat/montserrat_regular-webfont.woff2') format('woff2'),
          url('/fonts/montserrat/montserrat_regular-webfont.woff') format('woff'),
          url('/fonts/montserrat/montserrat_regular-webfont.ttf') format('truetype'),
          url('/fonts/montserrat/montserrat_regular-webfont.svg#montserratregular') format('svg');
  font-weight: normal;
  font-style: normal;
}

.font-montserratregular {
  font-family "montserratregular"
  speak none
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale
}

.font-montserratbold {
  font-family "montserratbold"
  speak none
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale
}