.leaflet-div-icon
  background-color:transparent
  border:none

.leaflet-popup-content
  background-color:transparent
  margin:0px

.leaflet-popup-tip-container
  display:none

.leaflet-top, .leaflet-right, .leaflet-bottom, .leaflet-left
  z-index 3

.leaflet-bottom
  margin-bottom 25px
  .leaflet-control
    &:last-child
      margin-bottom 0px
  &.leaflet-right
    margin-bottom 0px !important

.leaflet-marker-icon
  .circle-nr
    height 100%
    width 100%
    span
      height 100%
      width 100%
      font-size 14px
      line-height 30px//matchxs icon height


@import './leafletDraw'
