#!/bin/bash
set -e
set -o pipefail

SCRIPT_DIR="$( cd -P "$( dirname "${BASH_SOURCE[0]}" )" > /dev/null && pwd )/.."


source $SCRIPT_DIR/app/syncVars

if [[ "$IS_HEROKU" != "" ]]
then
    bin/start-pgbouncer-stunnel $SCRIPT_DIR/app/processJobQueueImpl wait
else
    $SCRIPT_DIR/app/processJobQueueImpl go
fi
