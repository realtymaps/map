#!/bin/bash

SCRIPT_DIR="$( cd -P "$( dirname "${BASH_SOURCE[0]}" )" > /dev/null && pwd )/.."


eval "export MAIN_DATABASE_URL=`$SCRIPT_DIR/database/getDbUrl MAIN_DATABASE`"
eval "export RAW_TEMP_DATABASE_URL=`$SCRIPT_DIR/database/getDbUrl RAW_TEMP_DATABASE`"
eval "export NORMALIZED_DATABASE_URL=`$SCRIPT_DIR/database/getDbUrl NORMALIZED_DATABASE`"
