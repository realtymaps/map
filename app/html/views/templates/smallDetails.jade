div( ng-controller='RealtyMaps-SmallDetailsCtrl' class="panel panel-primary" style="white-space:nowrap" ng-cloak)
  div(class="panel-heading") {{street_address_num}} {{street_address_name}}
  div(class="panel-body")
    section Price: ${{price}}
    section Beds: {{beds_total}}
    section Baths: {{baths_total}}
    section Year Built: {{year_built}}
    section Acres: {{acres}}